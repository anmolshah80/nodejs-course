<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Short URL</title>
  </head>
  <body>
    <ol>
      <% allUrls.forEach(url => {
        <li><%= url.shortId %> - <%= url.redirectURL %> - <%= url.visitHistory.length %> visits</li>
      }) %>
    </ol>
  </body>
</html>
